import java.util.Scanner;
public class allarthmetic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  Scanner input=new Scanner(System.in);
  System.out.println("welcome to arthmetic opertor");
  System.out.print("enter your first no.:");
  int first=input.nextInt();
  System.out.print("enter your second no.:");
  int second=input.nextInt();
  int add=first+second;
  int sub=first-second;
  int mul=first*second;
  int div=first/second;
  int mod=first%second;
  System.out.println("Add number is:" +add);
  System.out.println("sub number is:" +sub);
  System.out.println("mul number is:" +mul);
  System.out.println("div number is:" +div);
  System.out.println("modd number is:" +mod);
  
	}

}
  
