import java.util.Scanner;
public class perimeter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner input=new Scanner(System.in);
System.out.println("welcome to perimeter calculagtor");
System.out.print("enter your all 4 side no. :");
int a=input.nextInt();
int b=input.nextInt();
int c=input.nextInt();
int d=input.nextInt();
int perimeter=a+b+c+d;
System.out.println("perimeter rectangle number is :" +perimeter);
	}

}
